

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <string>
using namespace std;

string makeAbba(string s1, string s2) {
    return s1 + s2 + s2 + s1; // Concatenate s1 + s2 + s2 + s1
}

int main() {
    string s1, s2;
    cin >> s1 >> s2; // Read two strings as input
    
    // Output the result
    cout << makeAbba(s1, s2) << endl;
    
    return 0;
}
